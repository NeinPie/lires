<header class="header">
    <img class="logo" src="/img/oni.png" alt="Lires-Zwiebel" id="logo" onclick="redirectHome()">
    <input id="suchleiste" class="suchleiste">
    <button id="filbut" class="filbut" onclick="filBut()">
        <img src = "/img/fil.png" alt="Filter" width="55px">
    </button>

    <input type="text" id="recTitle" class="recTitle">
    <div class="recHeadInpList">
      <ul>
        <li><input placeholder="Zubereitungsdauer in Minuten"></li>
        <li><h3>Arbeitsaufwand: </h3>kaum<input type="range" min="1" max="5" value="2.5" id="aufwand">sehr hoch</li>
      </ul>
    </div>
    <button class="recSave">Speichern</button>

    <nav>
      <ul class="navLinks">
        <li class="tooltipCal">
          <a href="link"></a>
          <img class="navpic" src="/img/cal.png" alt="Kochkalender">
          <span class="tooltipCaltext">Kochen planen</span>
        </li>
        <li class="tooltipList" id="ListBut" onclick="listBut()">
          <a href="link"></a>
          <img class="navpic" src="/img/lists.png" alt="Listen">
          <span class="tooltipListtext">Zutaten in Einkaufslisten zusammenfassen</span>
        </li>
        <li>
          <a href="link"></a>
          <usrnm id="usrnm"><%= usrnm %></usrnm>
        </li>
        <li class="tooltipProfil">
          <a href="link"></a>
          <img class="navpic2" src="/img/bsppb.png" alt="beispielhaftes Profilbild">
          <span class="tooltipProfiltext">Zum Profil</span>
        </li>
    </ul>
    </nav>
</header>